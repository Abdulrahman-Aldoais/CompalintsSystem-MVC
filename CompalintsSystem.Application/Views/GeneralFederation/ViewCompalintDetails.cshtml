<div class="offcanvas offcanvas-end" id="rejectComplaint" aria-hidden="true">
    <div class="offcanvas-header border-bottom">
        <h6 class="offcanvas-title"> رفض الشكوى</h6>
        <button type="button" asp-action="RejectedThisComplaint" asp-controller="GeneralFederation"></button>
    </div>
    <div class="offcanvas-body flex-grow-1">
        <form asp-action="RejectedThisComplaint" asp-controller="GeneralFederation" method="post">


            <div class="mb-3">

                <label for="invoice-message" class="form-label">سبب رفض هذه الشكوى</label>
                <input type="hidden" asp-for="@Model.RejectedComplaintVM.UploadsComplainteId" />
                @*@Html.HiddenFor(m => m.RejectedComplaintVM.UploadsComplainteId)*@
                <textarea asp-for="@Model.RejectedComplaintVM.reume" class="form-control" cols="3" rows="4"></textarea>
                <span class="text-danger" asp-validation-for="@Model.RejectedComplaintVM.reume"></span>
            </div>

            <div class="d-flex flex-wrap mb-3">
                <button type="submit" class="btn btn-primary me-3" data-bs-dismiss="offcanvas">إرسال</button>
                <button type="button" class="btn btn-label-secondary" data-bs-dismiss="offcanvas">إالغاء</button>
            </div>
        </form>
    </div>
</div>

